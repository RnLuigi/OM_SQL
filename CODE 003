SELECT * FROM VIEW_ATENDIMENTO_F9_GNS
WHERE DATA LIKE '%08/2022%'


SELECT * FROM VIEW_BASE_PROTOCOLOS


SELECT 

DATA_REF                                        AS DATA             ,

((CAST(LPAD(SUBSTR("DATA/HORA DE ABERTURA",12,5),2) AS INT) *60 *60) +
(CAST(SUBSTR("DATA/HORA DE ABERTURA",15,2) AS INT) * 60)) 
                                                AS HORA             ,

LPAD("DATA/HORA DE FECHAMENTO",10)              AS DATA_FECHAMENTO  ,

((CAST(LPAD(SUBSTR("DATA/HORA DE FECHAMENTO",12,5),2) AS INT) *60 *60) +
(CAST(SUBSTR("DATA/HORA DE FECHAMENTO",15,2) AS INT) * 60)) 
                                                AS HORA_FECHAMENTO  ,

"DATA/HORA DE FECHAMENTO",

--DATA_REF, 
"AGENTE DE ORIGEM", 
"ALTO ATRITO", 
"BKO - IMPROCEDENTE", 
CEDENTE, 
CONTRATO, 
"AGENTE RESPONSAVEL", 
"COD AGENTE RESPONSAVEL", 
"NUMERO DO PROTOCOLO", 
"NOME DA CONTA CPF", 
"NAO CLIENTE", 
CPF, 
CENTRAL, 
--"DATA/HORA DE ABERTURA", 
--"DATA/HORA DE FECHAMENTO", 
"PROPRIETARIO PROTOCOLO", 
"CRIADO POR", 
"FECHADO PELO USUARIO", 
TIPO, 
MOTIVO, 
"SUB-MOTIVO", 
PRODUTO, 
"PRODUTO DO CONTRATO", 
"ORIGEM DO PROTOCOLO", 
STATUS, 
"CRIADO POR_", 
"PRIMEIRO ATENDIMENTO", 
EMAIL, 
CANAL

FROM TB_SALESFORCE_TODAS_CENTRAIS
